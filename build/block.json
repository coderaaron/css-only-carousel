{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "coderaaron/css-only-carousel",
  "version": "1.0.0",
  "title": "CSS Only Carousel",
  "category": "media",
  "icon": "gallery",
  "description": "A carousel that uses only CSS (and a tiny bit of JS for autoplay)",
  "supports": {
    "html": false,
    "align": [
      "wide",
      "full"
    ]
  },
  "attributes": {
    "id": {
      "type": "string",
      "default": ""
    },
    "selectedPosts": {
      "type": "array",
      "default": []
    },
    "linkThru": {
      "type": "boolean",
      "default": true
    },
    "editing": {
      "type": "boolean",
      "default": false
    },
    "autoPlay": {
      "type": "boolean",
      "default": false
    },
    "delay": {
      "type": "integer",
      "default": 6
    },
    "transition": {
      "type": "string",
      "default": "slide"
    },
    "showArrows": {
      "type": "boolean",
      "default": true
    },
    "dotType": {
      "type": "string",
      "default": "dots"
    }
  },
  "textdomain": "css-only-carousel",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "script": "file:./frontend.js"
}