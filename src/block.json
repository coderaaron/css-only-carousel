{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "coderaaron/css-only-carousel",
	"version": "1.0.0",
	"title": "CSS Only Carousel",
	"category": "media",
	"icon": "gallery",
	"description": "A carousel that uses only CSS",
	"supports": {
		"html": false
	},
	"attributes": {
		"selectedPosts": {
			"type": "array",
			"default": []
		},
		"linkThru": {
			"type": "boolean",
			"default": true
		},
		"editing": {
			"type": "boolean",
			"default": false
		},
		"delay": {
			"type": "integer",
			"default": 6
		},
		"showArrows": {
			"type": "boolean",
			"default": true
		},
		"dotType": {
			"type": "string",
			"default": "dots"
		}
	},
	"textdomain": "css-only-carousel",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
